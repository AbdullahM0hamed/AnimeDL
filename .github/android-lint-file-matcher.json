{
  "problemMatcher": [
    {
      "owner": "android-lint-file",
      "pattern": [
        {
          "regexp": "^(.+):\\s+(Warning|Error):\\s+(.+)\\s+\\[(.+)\\]$",
          "file": 1,
          "severity": 2,
          "message": 3,
          "code": 4,
          "kind": "file"
        }
      ]
    }
  ]
}
